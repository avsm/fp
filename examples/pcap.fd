format pcap_header {
    version_major: int16;
    version_minor: int16;
    timezone_offset: int32;
    timestamp_accuracy: int32;
    snap_len: int32;
    link_type: int32
}

format pcap_entry {
    sec: int32;
    usec: int32;
    cap_len: int32 value(sizeof(data));
    real_len: int32;
    data: byte[cap_len]
}

